{
  "name": "@liquidapps/dapp-client",
  "version": "0.0.1",
  "description": "JS library for interacting with DAPP network services.",
  "main": "dist/src/dapp-client-lib.js",
  "types": "dist/src/dapp-client-lib.d.ts",
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc",
    "build:live": "nodemon --watch '*.ts' --exec 'tsc' ./src/dapp-client-lib.js",
    "docs": "documentation readme dist/src/dapp-client-lib.js -s API",
    "test": "jest --coverage",
    "watch": "nodemon -e ts -w ./src -x npm run watch:serve",
    "watch:serve": "ts-node --inspect src/dapp-client-lib.ts",
    "posttest": "tslint -p ."
  },
  "directories": {
    "example": "examples"
  },
  "files": [
    "dist"
  ],
  "author": "Natanael Prudhomme",
  "license": "BSD 3-Clause",
  "devDependencies": {},
  "homepage": "https://liquidapps.io",
  "dependencies": {
    "@dfuse/client": "^0.3.0",
    "@types/debug": "^4.1.5",
    "@types/http-server": "^0.10.0",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "^24.0.25",
    "@types/node": "^12.7.5",
    "babel-polyfill": "^6.26.0",
    "bignumber.js": "^9.0.0",
    "documentation": "^12.1.4",
    "eosjs": "^20.0.0",
    "eosjs-ecc": "^4.0.7",
    "es5-shim": "4.5.13",
    "es6-shim": "0.35.5",
    "eslint": "^6.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.9.0",
    "js-sha256": "^0.9.0",
    "nodemon": "^1.19.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4",
    "ws": "^7.3.0"
  }
}
